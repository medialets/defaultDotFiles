[user]
    name = Jason Meredith
    email = jason.meredith@medialets.com

[alias]
    co = checkout

[apply]
    whitespace = nowarn

[core]
    editor = vim

[color]
    ui = true
[color]
    branch = auto
    diff = auto
    status = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
